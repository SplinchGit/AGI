{
  "app_name": "AGI Chat System",
  "version": "1.0.0",
  "ai_agents": {
    "claude": {
      "name": "Claude",
      "type": "claude",
      "api_key_env": "CLAUDE_API_KEY",
      "model": "claude-3-opus-20240229",
      "max_tokens": 4096,
      "temperature": 0.7
    },
    "james_clone": {
      "name": "James (Clone)",
      "type": "qwen",
      "api_key_env": "QWEN_API_KEY",
      "model": "qwen-plus",
